{
  "eslint.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.eol": "\n",
  "editor.defaultFormatter": "vscode.typescript-language-features",
  "editor.tabSize": 2,
  "typescript.tsdk": "node_modules/typescript/lib"
}
